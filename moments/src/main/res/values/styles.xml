    <!-- Base application theme. -->
    <style name="ActivityTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/global_bg</item>
        <item name="android:windowFullscreen">false</item>
        <item name="no_data_img_tink">@color/no_data_img_tink</item>
        <item name="selectableItemBackground">@drawable/abc_item_background_holo_dark</item>
        <item name="borderlessButtonStyle">@style/Widget.MaterialComponents.Button.TextButton</item>
    </style>

    <style name="Widget.SeekBar.Paint" parent="@android:style/Widget.SeekBar">
        <item name="android:maxHeight">3.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:minHeight">3.0dip</item>
    </style>
    <style name="GraffitiButton">
        <item name="android:textSize">@dimen/mainGraffitiButtonTextSize</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:paddingTop">@dimen/mainGraffitiButtonPaddingTop</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#ff181e2a</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:drawablePadding">-5dp</item>
    </style>

    <style name="DayText">
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="MeText">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="CircleNickName">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black333</item>
    </style>

    <style name="CircleContent">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ContentText">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black666</item>
    </style>

    <style name="AvatarText">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="UserZoneText">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="MeEditText">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black666</item>
    </style>

    <style name="DeleteCircleText">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/black333</item>
    </style>

    <style name="TabButton">
        <item name="android:textSize">11.0dip</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/tabtext</item>
        <item name="android:paddingTop">@dimen/mainGraffitiButtonPaddingTop</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:paddingBottom">2.0dp</item>
    </style>

    <style name="EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColorHint">@color/line</item>
    </style>

    <style name="CommonButton">
        <item name="android:textSize">17sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/shape_radiusbtn</item>
        <item name="android:textColor">@color/white</item>
    </style>

